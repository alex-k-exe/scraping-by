{
  "name": "scraping-by",
  "version": "0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "test": "vitest",
    "d1:gen": "drizzle-kit generate:sqlite --config=drizzle.config.ts",
    "d1:mig:local": "wrangler d1 migrations apply cloudflare_fullstack_db --local",
    "d1:mig:prod": "wrangler d1 migrations apply cloudflare_fullstack_db ",
    "wrangler": "wrangler pages dev ./.svelte-kit/cloudflare  --env staging  "
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250617.0",
    "@eslint/compat": "^1.3.0",
    "@eslint/js": "^9.29.0",
    "@sveltejs/kit": "^2.21.5",
    "@sveltejs/vite-plugin-svelte": "^5.1.0",
    "@tailwindcss/vite": "^4.1.10",
    "@types/node": "^22.15.32",
    "drizzle-kit": "^0.31.1",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-svelte": "^3.9.2",
    "flowbite-svelte": "1.1.0",
    "flowbite-svelte-icons": "^2.2.1",
    "globals": "^16.2.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "svelte": "^5.34.3",
    "svelte-check": "^4.2.1",
    "svelte-lib-helpers": "^0.4.30",
    "tailwindcss": "^4.1.10",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^6.3.5",
    "vitest": "^3.2.3"
  },
  "dependencies": {
    "@sveltejs/adapter-cloudflare": "^7.0.4",
    "drizzle-orm": "^0.43.1",
    "puppeteer": "^24.10.1"
  }
}
